# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
        . ~/.bashrc
fi

# User specific environment and startup programs

for f in $HOME/.dotfiles/*; do
   . $f
done

for f in $HOME/.secrets/*; do
   . $f
done

